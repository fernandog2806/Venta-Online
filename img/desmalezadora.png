    function addToCart()
    {
        //$('.js-buy-btn').spin();
        uiloader_show('Aguarde unos instantes...');
        $('.js-buy-btn').attr('disabled', true);
        var cart = $('#add-to-cart');

        $.post(
                cart.attr('action'),
                cart.serialize(),
                function(data)
                {
                    if(data.message != undefined)
                    {
                        if (data.status != undefined)
                        {
                            if (data.status == 'added' || data.status == 'already_added')
                            {
                                displayCartItemsBox();
                            }
                        }
                        else
                        {
                            var str;

                            str = data.message+' <a href="https://www.ferreterasanluis.com/checkout"> Ver carro</a>';

                            UIkit.notify(str, { status  : 'success', timeout : 5000, pos : 'top-center' });
                        }

                        //$('#productAlerts').html('<div class="alert green">'+data.message+' <a href="https://www.ferreterasanluis.com/checkout"> Ver carro</a> <i class="close"></i></div>');
                        updateItemCount(data.itemCount, data.grandTotal);
                        cart[0].reset();
                    }
                    else if(data.error != undefined)
                    {
                        //$('#productAlerts').html('<div class="alert red">'+data.error+' <i class="close"></i></div>');
                        UIkit.notify(data.error/*+' <a href="https://www.ferreterasanluis.com/checkout"> Ver carro</a>'*/, { status  : 'info', timeout : 5000, pos : 'top-center' });
